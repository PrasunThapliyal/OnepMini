<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "OnepMini" namespace = "OnepMini.OrmNhib.Migrations.HBM">
  <class name="NHMigrationsHistory" table="nh_migrations_history" lazy="true">
    <id name = "Id" column="oid">
      <generator class = "hilo"/>
    </id>

    <property name="Name" column="name" />
    <property name="OnepBackendVersion" column="onepBackendVersion" />
    <property name="PreviousOnepBackendVersion" column="previousOnepBackendVersion" />
    <property name="Notes" column="notes" />
    <property name="CreationTime" column="creationTime" />

  </class>
</hibernate-mapping>
{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "Location",
    "type": "object",
    "properties": {
        "mgmtAccess": {
            "description": "Different type presentation of a Location",
            "type": "string",
            "enum": [
                "TL1_ACCESS",
                "SAOS_ACCESS"
            ]
        },
        "address": {
            "description": "List of attributes to describe a Location",
            "type": "array",
            "items": {
                "type": "object",
                "$ref": "LocationAddress.json"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "mgmtAccess", "address"
    ]
}

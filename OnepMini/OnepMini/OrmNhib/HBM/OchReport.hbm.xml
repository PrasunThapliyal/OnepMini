<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "OnepMini" namespace = "TopologyRestLibrary.V1.Etp.Reports">

  <class name="OchReport" table="och_report" lazy="true" >
    <id name = "OId" column="oid">
      <generator class = "hilo"/>
    </id>



    <bag name="Data" lazy="false" cascade="all-delete-orphan" inverse="false" fetch="select">
        <key column="ochReport" not-null="true" update="false" foreign-key="och_report_data_FK_ochReport" ></key>
        <one-to-many class="OchReportData"></one-to-many>
    </bag>



  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "OnepMini" namespace = "OnepMini.V1.Etp.Common">

  <class name="PlanningState" table="planning_state" lazy="true" >
    <id name = "OId" column="oid">
      <generator class = "hilo"/>
    </id>

    <property name="PlanningStatus" column="planningStatus"  />
    <property name="PlanningSource" column="planningSource"  />
    <property name="PlanningTimestamp" column="planningTimestamp"  />


    <many-to-one name="PlanningPhase" column="planningPhase" unique="true" cascade="all-delete-orphan" lazy="no-proxy" />





  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "OnepMini" namespace = "TopologyRestLibrary.V1.Etp.Reports">

  <class name="OchReportData" table="och_report_data" lazy="true" >
    <id name = "OId" column="oid">
      <generator class = "hilo"/>
    </id>

    <property name="Id" column="id"  />
    <property name="Type" column="type"  not-null="true"  />


    <many-to-one name="Attributes" column="attributes" unique="true" cascade="all-delete-orphan" lazy="no-proxy" />





  </class>
</hibernate-mapping>
